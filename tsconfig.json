{
    "compilerOptions": {
        "noImplicitAny": true,
        "noEmitOnError": true,
        "strictNullChecks": true,
        "target": "es5",
        "module": "esnext",
        "declaration": true,
        "outDir": "./lib",
        "strict": true,
        "sourceMap": true,
        "esModuleInterop": true,
        "moduleResolution":"node",
    },
    "include": ["src"],
    "exclude": ["node_modules", "test", "build"]
